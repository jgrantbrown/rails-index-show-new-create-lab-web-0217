<p id="notice"><%= notice %></p>

<h1>Listing Coupons</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupons.each do |coupon| %>

      <tr>
        <p> <%= coupon.store %> <p>
        <td><%= link_to coupon.coupon_code, coupon_path(coupon) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupon', new_coupon_path %>
